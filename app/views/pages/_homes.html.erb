<section class="homes">
  <div class="home">
    <%= image_tag asset_pack_path('img/house-1.jpeg'),
      alt: 'House 1',
      class: 'home__img'
    %>
    <svg class="home__like">
      <use xlink:href="<%= asset_pack_path('img/sprite.svg') %>#icon-heart-full"></use>
    </svg>
    <h5 class="home__name">Beautiful Family House</h5>
    <div class="home__location">
      <svg>
        <use xlink:href="<%= asset_pack_path('img/sprite.svg') %>#icon-map-pin"></use>
      </svg>
      <p>USA</p>
    </div>
    <div class="home__rooms">
      <svg>
        <use xlink:href="<%= asset_pack_path('img/sprite.svg') %>#icon-profile-male"></use>
      </svg>
      <p>5 rooms</p>
    </div>
    <div class="home__area">
      <svg>
        <use xlink:href="<%= asset_pack_path('img/sprite.svg') %>#icon-expand"></use>
      </svg>
      <p>3500 ft<sup>2</sup></p>
    </div>
    <div class="home__price">
      <svg>
        <use xlink:href="<%= asset_pack_path('img/sprite.svg') %>#icon-key"></use>
      </svg>
      <p>$1,200,000</p>
    </div>
    <button class="btn home__btn">Contact realtor</button>
